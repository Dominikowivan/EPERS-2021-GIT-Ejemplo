Class Golondrina {

 int energia;
 string locacion;
 string arma;
 int ataque;


 public void comer(int unaCantidadDeComida){
   this.energia = this.energia + unaCantidadDeComida
 }

  public void equipar(string unArma){
    this.arma = unArma
  }

  public void agregarAtaque(int unaCantidadDeAtaque){
      this.ataque = this.ataque + unaCantidadDeAtaque
    }

}