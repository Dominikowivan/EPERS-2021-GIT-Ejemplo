Class Golondrina {

 int energia;
 string locacion;
 string arma;
 int mana;

 public void comer(int unaCantidadDeComida){
   this.energia = this.energia + unaCantidadDeComida
 }


/// PAra volar como precondicion una golondrina
// tiene que poder tener la energia necesaria para moverse

 public void volar(string unaLocacion, int kilometrosDeDistancia){
    if (energia > kilometrosDeDistancia)
        this.locacion = unaLocacion
         this.energia = this.energia - kilometrosDeDistancia
    else
        throw NosePuedeMoverALocacion(unaLocacion)
 }

 public void cambiarDeEstado(string estado){
    this.estado = estado
 }
  public void equipar(string unArma){
    this.arma = unArma
  }

 public void invocarBolaDeFuego(int unaCantidadDeMana){
    this.mana = this.mana - unaCantidadDeMana
  }

  public void nadar(string unaLocacion, int kilometrosDeDistancia ){
     this.locacion = unaLocacion
  }

}